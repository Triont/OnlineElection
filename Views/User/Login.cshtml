@model OnlineElection.ModelView.LoginUser
@{
    var returnUrl = @Context.Request.Query["returnurl"];
}





<form asp-controller="User" asp-action="Login" asp-route-returnurl="@returnUrl" asp-action="post">
<div asp-validation-summary=All></div>
<label asp-for="Email">
    Your email

</label>    
<input type ="email" asp-for="Email" />

<label asp-for="Password">
    Your password

</label>    
<input type ="password" asp-for="Password" />

<input type="submit" />

</form>

<a href="User/Forgotten">Forgot password</a>


